{
  "name": "compgenrc",
  "description": "generate react nextjs and react native component",
  "version": "1.0.1",
  "author": "Sambare Aboubacar",
  "bin": {
    "compgenrc": "./bin/run"
  },
  "bugs": "https://github.com/boubaSambare/compgen/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "chalk": "^4.1.1",
    "conf": "^10.0.1",
    "inquirer": "^8.1.0",
    "inquirer-directory": "^2.2.0",
    "node-plop": "^0.26.2",
    "plop": "^2.7.4",
    "shelljs": "^0.8.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/conf": "^3.0.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/shelljs": "^0.8.8",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "husky": "^6.0.0",
    "mocha": "^9.0.0",
    "nyc": "^14",
    "semantic-release": "^17.4.4",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/boubaSambare/compgen/pakages/compgen",
  "keywords": [
    "oclif",
    "react",
    "react native",
    "nextjs",
    "generator",
    "component",
    "compgenrc"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "compgenrc",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-autocomplete"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>.",
      "registry": "https://registry.npmjs.org/compgenrc"
    }
  },
  "repository": "boubaSambare/compgenrc",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme && cp -r src/templates lib",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "semantic-release": "semantic-release",
    "pre-commit": "npm test"
  },
  "types": "lib/index.d.ts"
}
