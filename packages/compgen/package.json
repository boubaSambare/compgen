{
  "name": "compgen",
  "description": "generate react or react native component",
  "version": "0.0.1",
  "author": "Sambare Aboubacar",
  "bin": {
    "compgen": "./bin/run"
  },
  "bugs": "https://github.com/boubaSambare/compgen/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "chalk": "^4.1.1",
    "enquirer": "^2.3.6",
    "inquirer": "^8.1.0",
    "inquirer-directory": "^2.2.0",
    "node-plop": "^0.26.2",
    "plop": "^2.7.4",
    "shelljs": "^0.8.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/shelljs": "^0.8.8",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/boubaSambare/compgen",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "compgen",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "boubaSambare/compgen",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
